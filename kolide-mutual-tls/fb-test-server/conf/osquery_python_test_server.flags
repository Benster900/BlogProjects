--host_identifier=hostname

--tls_hostname=kolide.hackinglab.local:8080
--tls_server_certs=conf/tls/root_ca/root_CA.crt

--tls_client_cert=conf/tls/client/device01_devices_hackinglab_local.crt
--tls_client_key=conf/tls/client/device01_devices_hackinglab_local.key

--enroll_secret_path=fb-test-server/tests/test_enroll_secret.txt
--enroll_tls_endpoint=/enroll

--config_plugin=tls
--config_tls_endpoint=/config
--config_refresh=10


--disable_distributed=false
--distributed_plugin=tls
--distributed_interval=10
--distributed_tls_max_attempts=3
--distributed_tls_read_endpoint=/distributed_read
--distributed_tls_write_endpoint=/distributed_write

--logger_plugin=tls
--logger_tls_endpoint=/log
--logger_tls_period=10
